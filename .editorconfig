# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*.{config,resx,settings}]
end_of_line = lf
indent_style = space
indent_size = 2

[*.cs]
end_of_line = lf
indent_style = space
indent_size = 4

[*.csproj]
end_of_line = crlf
indent_style = space
indent_size = 2

